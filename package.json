{
  "name": "alekseew-arduino",
  "version": "0.4.1",
  "description": "Get data from arduino through the serial port and setup the bobby-car race that is hold in Bammental - Germany.",
  "keywords": [
    "arduino",
    "node",
    "nodejs",
    "typescript",
    "serialport",
    "bobby-car",
    "race",
    "bammental"
  ],
  "author": "Michael Alekseew <alekseew.m@googlemail.com>",
  "repository": "https://github.com/Mischa1610/arduino-bobbycar-race-bammental",
  "homepage": "https://bobbycar-race-bammental.web.app",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.15.0",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "ng": "nx",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "concurrently -k -p '[{name}]' -n 'Backend-Nest,Frontend-Angular' -c 'magenta.bold,cyan.bold' 'npm run start-be' 'npm run start-fe'",
    "start-be": "nx serve api",
    "start-fe": "nx serve bobbycar-race-bammental --host 0.0.0.0",
    "build": "npm run build-be & npm run build-fe",
    "build-be": "nx build api --prod",
    "build-fe": "nx build bobbycar-race-bammental --prod",
    "test": "npm run test-be && npm run test-fe",
    "test-be": "nx test api --code-coverage",
    "test-fe": "nx test bobbycar-race-bammental --code-coverage",
    "lint": "nx workspace-lint && npm run lint-be && npm run lint-fe",
    "lint-be": "nx lint api",
    "lint-fe": "nx lint bobbycar-race-bammental",
    "format": "nx format",
    "format-check": "nx format:check"
  },
  "dependencies": {
    "@angular/animations": "17.0.5",
    "@angular/cdk": "17.0.2",
    "@angular/common": "17.0.5",
    "@angular/compiler": "17.0.5",
    "@angular/core": "17.0.5",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "17.0.5",
    "@angular/material": "17.0.2",
    "@angular/platform-browser": "17.0.5",
    "@angular/platform-browser-dynamic": "17.0.5",
    "@angular/router": "17.0.5",
    "@nestjs/common": "10.2.10",
    "@nestjs/core": "10.2.10",
    "@nestjs/platform-express": "10.2.10",
    "@ngneat/transloco": "^4.1.0",
    "@nrwl/angular": "17.1.3",
    "flag-icons": "^6.6.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.1",
    "tslib": "^2.0.0",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.5",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "^14.0.2",
    "@angular/compiler-cli": "17.0.5",
    "@angular/language-service": "17.0.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/schematics": "10.0.3",
    "@nestjs/testing": "10.2.10",
    "@nrwl/cli": "~14.3.6",
    "@nrwl/cypress": "17.1.3",
    "@nrwl/eslint-plugin-nx": "17.1.3",
    "@nrwl/jest": "17.1.3",
    "@nrwl/linter": "17.1.3",
    "@nrwl/nest": "17.1.3",
    "@nrwl/node": "17.1.3",
    "@nrwl/nx-cloud": "16.5.2",
    "@nrwl/workspace": "17.1.3",
    "@types/jest": "29.4.4",
    "@types/node": "~16.11.7",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "concurrently": "^7.2.2",
    "cypress": "^9.1.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.1",
    "husky": "^8.0.1",
    "jest": "29.4.3",
    "jest-preset-angular": "13.1.4",
    "lint-staged": "^13.0.2",
    "nx": "17.1.3",
    "prettier": "^2.6.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2",
    "jest-environment-jsdom": "28.1.3"
  },
  "lint-staged": {
    "*.js,*.ts,*.html,*.scss,*.css,*.json": "nx workspace-lint --fix && nx lint api --fix && nx lint bobbycar-race-bammental --fix",
    "*": "prettier --ignore-unknown --write"
  }
}
