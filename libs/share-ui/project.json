    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/share-ui/**/*.ts", "libs/share-ui/**/*.html"]
      }
    }
  },
  "tags": ["ui"]
}
